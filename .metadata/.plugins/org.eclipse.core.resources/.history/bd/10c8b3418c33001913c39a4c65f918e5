package test1;

public class function {

    public static void main(String[] args) {
        double a = 0;
        double b = 0;
        double c = 0;
        
        double D = b*b - (4*a*c);
		double x = Math.sqrt(D);

        if (D > 0) {
        	if ((-b+x) == 0) {
    			System.out.println("x1=");
    			System.out.println("x2=");
        	}
        	else {
    			System.out.print("x1=");
    			System.out.println((-b+x)/(2*a));
    			System.out.print("x2=");
    			System.out.println((-b-x)/(2*a));
        	}

		}
		else if (D == 0) {
			System.out.print("x1=");
			System.out.println(-b/(2*a));
			System.out.print("x2=");
			System.out.println(-b/(2*a));

		}
		else {
			System.out.println("x1=");
			System.out.println("x2=");
		}
        
    }
}
