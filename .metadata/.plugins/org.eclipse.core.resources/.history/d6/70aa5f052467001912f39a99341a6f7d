package ua.lviv.iot.cutlery.models;

public class SpoonCutlery extends Cutlery {
	
	private int lenght;
	private String colors;

	public SpoonCutlery() {}

	public SpoonCutlery(double rating, double price, String material, String production, int lenght, String colors) {
		super(rating, price, material, production);
		this.lenght = lenght;
		this.colors = colors;

	}


	public int getLenght() {
		return lenght;
	}

	public void setLenght(int lenght) {
		this.lenght = lenght;
	}

	public String getColors() {
		return colors;
	}

	public void setColor(String colors) {
		this.colors = colors;
	}

	@Override
	public String toString() {
		return "\n" + super.toString() + "SpoonCutlery [lenght=" + lenght + ", colors=" + colors + "]";
	}
	
	

}
